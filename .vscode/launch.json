{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Main",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "npx",
            "runtimeArgs": [
                "ts-node",
                "dist/server.js"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "env": {
                "PORT": "3000",
                "REDIS_HOST": "127.0.0.1",
                "REDIS_PORT": "6379",
                "INFERENCE_HOST": "127.0.0.1",
                "INFERENCE_PORT": "11434",
                "ELASTIC_HOST": "127.0.0.1",
                "ELASTIC_PORT": "5000"
            }
        },
        {
            "name": "Run Consumer",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "npx",
            "runtimeArgs": [
                "ts-node",
                "dist/queue/server.js"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "env": {
                "REDIS_HOST": "127.0.0.1",
                "REDIS_PORT": "6379",
                "INFERENCE_HOST": "127.0.0.1",
                "INFERENCE_PORT": "11434",
                "ELASTIC_HOST": "127.0.0.1",
                "ELASTIC_PORT": "5000"
            }
        },
        {
            "name": "Run Prometheus",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "npx",
            "runtimeArgs": [
                "ts-node",
                "dist/metrics/prometheus/server.js"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "env": {
                "REDIS_HOST": "127.0.0.1",
                "REDIS_PORT": "6379",
                "INFERENCE_HOST": "127.0.0.1",
                "INFERENCE_PORT": "11434",
                "PORT": "9095",
                "ELASTIC_HOST": "127.0.0.1",
                "ELASTIC_PORT": "9200"
            }
        },
        {
            "name": "Run Elastic",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "npx",
            "runtimeArgs": [
                "ts-node",
                "dist/metrics/elastic.js"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "env": {
                "REDIS_HOST": "127.0.0.1",
                "REDIS_PORT": "6379",
                "INFERENCE_HOST": "127.0.0.1",
                "INFERENCE_PORT": "11434",
                "ELASTIC_HOST": "127.0.0.1",
                "ELASTIC_PORT": "9200",
                "PORT": "5000"
            }
        }
    ]
}